.PHONY: biff like-a-tree

like-a-tree: biff boondocks

biff:
	@echo "\n\nYOUNG BIFF:\n\tWhy don't you make like a tree and get out of here?\n\nOLD BIFF:\n\tIt's LEAVE, you idiot! \"Make like a tree and leave.\"\n\tYou sound like a damn fool when you say it wrong.\n\n\t\t\t\t\t\t(Back To The Future II)\n\n"; sleep 4

boondocks:
	@echo "\n\nDOC:\n\tWhy don't you make like a tree, and get the fuck outta here?\n\n\t\t\t\t\t\t(The Boondock Saints)\n\n"; sleep 4
